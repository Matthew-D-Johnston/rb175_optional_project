      <ul>
        <li>Adj. EPS:</li>
        <li>Revenue:</li>
      </ul>
    <% when "Q4" %>
      <p><b>Q3 year</b></p>
      <ul>
        <li>Adj. EPS:</li>
        <li>Revenue:</li>
