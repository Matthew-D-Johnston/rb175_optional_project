      <ul>
        <li>Adj. EPS:</li>
        <li>Revenue:</li>
        <li>Suprise?:</li>
        <li>Stock:</li>
      </ul>
      <p><b>Q1 year Estimate</b></p>
      <ul>
        <li>Adj. EPS:</li>
        <li>Revenue:</li>
      </ul>
    <% when "Q2" %>
      <p><b>Q1 year</b></p>
      <ul>
        <li>Adj. EPS:</li>
        <li>Revenue:</li>
        <li>Suprise?:</li>
        <li>Stock:</li>
      </ul>
      <p><b>Q4 prior year</b></p>
      <ul>
        <li>Adj. EPS:</li>
        <li>Revenue:</li>
        <li>Suprise?:</li>
        <li>Stock:</li>
      </ul>
      <p><b>Q2 year Estimate</b></p>
      <ul>
        <li>Adj. EPS:</li>
        <li>Revenue:</li>
      </ul>
    <% when "Q3" %>
      <p><b>Q2 year</b></p>
      <ul>
        <li>Adj. EPS:</li>
        <li>Revenue:</li>
        <li>Suprise?:</li>
        <li>Stock:</li>
      </ul>
      <p><b>Q1 year</b></p>
      <ul>
        <li>Adj. EPS:</li>
